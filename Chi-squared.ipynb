   "source": [
    "#同じ比率でサンプル数が100倍違う2×2のクロス表\n",
    "x <- matrix(1:4, 2,2)\n",
    "x[1,1]<-10\n",
    "x[1,2]<-20\n",
    "x[2,1]<-35\n",
    "x[2,2]<-45\n",
    "x\n",
    "y<- matrix(1:4, 2,2)\n",
    "y[1,1]<-1000\n",
    "y[1,2]<-2000\n",
    "y[2,1]<-3500\n",
    "y[2,2]<-4500\n",
    "y"
   ]
  },
